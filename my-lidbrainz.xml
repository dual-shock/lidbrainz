<?xml version="1.0"?>
<Container version="2">
  <Name>lidbrainz</Name>
  <Repository>dualshock/lidbrainz:dev</Repository>
  <Registry>https://hub.docker.com/r/dualshock/lidbrainz</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/dual-shock/lidbrainz</Support>
  <Project>https://github.com/dual-shock/lidbrainz</Project>
  <Overview>search bar for musicbrainz to send to lidarr</Overview>
  <Category>Tools: Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8080]/health</WebUI>
  <TemplateURL>false</TemplateURL>
  <Icon>https://raw.githubusercontent.com/dual-shock/lidbrainz/refs/heads/main/interface/assets/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WebUI Port" Target="8080" Default="22000" Mode="tcp" Description="Port of the web ui" Type="Port" Display="always" Required="true" Mask="false">22000</Config>
  <Config Name="lidarr instance URL" Target="LIDARR_URL" Default="http://localhost:8686" Mode="" Description="url of your lidarr instance" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="lidarr instance API key" Target="LIDARR_APIKEY" Default="" Mode="" Description="API key for your lidarr instance" Type="Variable" Display="always" Required="true" Mask="true"></Config>
  <Config Name="custom musicbrainz user agent" Target="MUSICBRAINZ_USERAGENT" Default="" Mode="" Description="User agent for your personal instance" Type="Variable" Display="always" Required="true" Mask="false"></Config>
</Container>
